{
  "name": "GitHubGo",
  "version": "0.0.1",
  "description": "",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "gh"
  },
  "icons": {
    "16": "github-icon-16.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ]
}
