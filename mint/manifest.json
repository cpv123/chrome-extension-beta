{
  "name": "GitHubGo",
  "version": "2.0.1",
  "description": "A Github Chrome extension built with Mint",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "gh"
  },
  "icons": {
    "16": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ]
}