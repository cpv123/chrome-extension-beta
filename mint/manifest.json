{
  "name": "GitHubGo",
  "version": "2.0.1",
  "description": "A Github Chrome extension built with Mint",
  "manifest_version": 3,
  "omnibox": {
    "keyword": "gh"
  },
  "icons": {
    "16": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ]
}